<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.miketech.carrieredit">

    <application
        android:name="it.miketech.carrieredit.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="it.miketech.carrieredit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="xposedmodule"
            android:value="true"/>
        <meta-data
            android:name="xposeddescription"
            android:value="更改运营商名称 \n开发者：周毅刚"/>
        <meta-data
            android:name="xposedminversion"
            android:value="54"/><!-- 对应的XposedBridge版本号 -->

    </application>

</manifest>